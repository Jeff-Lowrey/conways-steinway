# macOS
.DS_Store
.AppleDouble
.LSOverride

# VSCode specific patterns
**/.vscode
.vscode/
.vscode/*
!.vscode/extensions.json
!.vscode/launch.json
!.vscode/tasks.json

# Rust-specific patterns are in the rust/.gitignore file
# Do not add Rust-specific patterns here - add them to rust/.gitignore instead

# Python-specific patterns are in the python/.gitignore file
# Do not add Python-specific patterns here - add them to python/.gitignore instead

# Exception: __pypackages__ can appear in config/ directory
config/__pypackages__/


# Audio samples - large binary files (but include piano samples needed for the app)
*.mp3
*.aiff
*.flac
*.ogg

# Include piano samples - essential for the application
!samples/
!samples/*.wav
!static/audio/
!static/audio/*.wav
!*.wav

# IDE files
.idea/
*.swp
*.swo
*~
.vs/
.project
.classpath
.settings/
*.sublime-workspace
*.sublime-project

# Build artifacts
*.exe
*.so
*.dll
*.dylib

# Temporary files
*.tmp
*.temp
*.log
tmp/
temp/
.cache/

# OS generated files
Thumbs.db
ehthumbs.db
Desktop.ini
